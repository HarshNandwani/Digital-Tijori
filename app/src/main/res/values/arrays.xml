<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Everytime a company is added in assets/companies.json add
        the icon resource id in this array at bottom -->
    <integer-array name="company_icons">
        <item>@drawable/airtel_icon</item>
        <item>@drawable/axis_bank_icon</item>
        <item>@drawable/hdfc_bank_icon</item>
        <item>@drawable/icici_bank_icon</item>
        <item>@drawable/paytm_icon</item>
        <item>@drawable/sbi_icon</item>
    </integer-array>

    <!-- Everytime a company is added in assets/companies.json add
    the logo resource id in this array at bottom, if it doesn't have a logo then -1 -->
    <integer-array name="company_logos">
        <item>-1</item>
        <item>@drawable/axis_bank_logo</item>
        <item>@drawable/hdfc_bank_logo</item>
        <item>@drawable/icici_bank_logo</item>
        <item>-1</item>
        <item>@drawable/sbi_logo</item>
    </integer-array>

</resources>
